  {:block/id #uuid "656ae861-aa84-4ad2-9233-103bfffa53ab",
   :block/page-name "Inhaltserstellung",
   :block/properties nil,
   :block/children
   ({:block/id #uuid "65084207-a30a-4148-935a-b0701cf53f25",
     :block/properties nil,
     :block/format :markdown,
     :block/children
     [{:block/id #uuid "65084207-d33d-426b-bb2b-253ac3b6c08a",
       :block/properties {:type "toc", :chapter 11},
